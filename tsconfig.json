{
	"compilerOptions": {
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"target": "ESNext",
		"skipLibCheck": true,
		"lib": [ "ES2022" ],
		"types": [ "hackmud-script-manager/env" ],
		"strict": true,
		"paths": { "/*": [ "./src/*" ] },
        "outDir": "./dist",
        "sourceMap": true,
        "inlineSourceMap": false,
        "declaration": true,
	}
}
